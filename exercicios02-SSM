programa
{
funcao inicio()
{

inteiro numero1, numero2, soma, subtracao, multiplicacao

numero1=1
numero2=2

escreva("\n Digite o primeiro número: ")

leia(numero1)

escreva("\n Digite o segundo número: ")

leia(numero2)

inteiro soma <- numero1 + numero2
inteiro subtracao <- numero1 - numero2
inteiro multiplicacao <- numero1 * numero2
escreva("\n A soma dos números é: ", soma)
escreva("\n A subtração dos números é: ", subtracao)
escreva("\n A multiplicação dos números é: ", multiplicacao)

        }
}
